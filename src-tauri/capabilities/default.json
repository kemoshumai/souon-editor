{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "opener:default",
    "shell:allow-open",
    "dialog:allow-open",
    "fs:scope-applocaldata",
    "fs:allow-copy-file",
    "fs:write-all",
    "fs:allow-applocaldata-write-recursive"
  ]
}
